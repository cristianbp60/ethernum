use array::ArrayTrait;

#[derive(Component)]
struct Position {
    x: u32,
    y: u32
}

trait PositionTrait {
    // fn is_zero(self: Position) -> bool;
    fn is_equal(self: Position, b: Position) -> bool;
}

impl PositionImpl of PositionTrait {
    // fn is_zero(self: Position) -> bool {
    //     match self.x - self.y {
    //         0 => false,
    //         _ => true,
    //     }
    // }

    fn is_equal(self: Position, b: Position) -> bool {
        self.x == b.x & self.y == b.y
    }
}
